<scenefile>
    <globaldata>
  <diffusecoeff v="0.5"/>
  <specularcoeff v="0.5"/>
  <ambientcoeff v="0.5"/>
    </globaldata>

    <cameradata>
  <pos x="6" y="4" z="6"/>
  <focus x="0" y="1" z="0"/>
  <up x="0" y="1" z="0"/>
  <heightangle v="45"/>
    </cameradata>

    <lightdata>
  <id v="0"/>
  <color r="1" g="1" b="1"/>
  <position x="4" y="4" z="4"/>
    </lightdata>

    <lightdata>
  <id v="1"/>
  <color r="1" g="1" b="1"/>
  <position x="-3" y="3" z="3"/>
    </lightdata>

    <lightdata>
  <id v="2"/>
  <color r="1" g="1" b="1"/>
  <position x="-3" y="3" z="-3"/>
    </lightdata>

    <lightdata>
  <id v="3"/>
  <color r="1" g="1" b="1"/>
  <position x="3" y="3" z="-3"/>
    </lightdata>

    

  <object type="tree" name="root">
   <!-- Hat -->

    <!-- Top of the hat -->
    <transblock>
        <translate x="0" y="3.25" z="0" />
        <rotate angle="-90" x="1" y="0" z="0"/> 
        <scale x="1.1" y="0.75" z="1.1" />
        <object type="primitive" name="cylinder">
            
            <diffuse r="0" g="0" b="0" a="1"/>
            <ambient r="0" g="0" b="0" a="1"/>
            <specular r="0" g="0" b="0" a="1"/>
        </object>
    </transblock>

    <!-- Rim  -->
    <transblock>
        <translate x="0" y="3.0" z="0" /> 
        <rotate angle="-90" x="1" y="0" z="0"/> 
        <scale x="1.5" y="0.1" z="1.5" />
        <object type="primitive" name="cylinder">
            
            <diffuse r="0" g="0" b="0" a="1"/>
            <ambient r="0" g="0" b="0" a="1"/>
            <specular r="0" g="0" b="0" a="1"/>
        </object>
    </transblock>
    <!-- Scarf -->
    <transblock>
        <translate x="0" y="2.2" z="0" /> 
        <rotate angle="-90" x="1" y="0" z="0"/> 
        <scale x="1.0" y="0.3" z="1.0" />
        <object type="primitive" name="cylinder">
            <diffuse r="0.5" g="0" b="0" a="1"/> 
            <ambient r="0.3" g="0" b="0" a="1"/> 
            <specular r="0.2" g="0" b="0" a="1"/>
        </object>
    </transblock>

<transblock>
    <translate x="0.5" y="1.6" z="0.5"/> 
    <rotate angle="-90" x="0" y="0" z="1"/> 
    <rotate angle="15" x="1" y="0" z="0"/> 
    <rotate angle="10" x="0" y="1" z="0"/> 
    <scale x="0.2" y="1.0" z="0.2"/> 
    <object type="primitive" name="cylinder">
        <diffuse r="0.5" g="0" b="0" a="1"/> 
        <ambient r="0.3" g="0" b="0" a="1"/> 
        <specular r="0.2" g="0" b="0" a="1"/>
    </object>
</transblock>





    <!-- Bottom sphere (body) -->
<transblock>
    <scale x="2" y="2" z="2"/>
    <object type="primitive" name="sphere">
        <diffuse r="0.9" g="0.9" b="0.9" a="1"/>
        <ambient r="0.9" g="0.9" b="0.9" a="1"/>
        <specular r="1" g="1" b="1" a="1"/>
    </object>
</transblock>


   <transblock>
            <translate x="0" y="1.5" z="0"/> <!-- Move the middle sphere above the bottom sphere -->
            <object type="tree" name="snowman_middle">
                <!-- Middle Sphere -->
                <transblock>
                    <scale x="1.5" y="1.5" z="1.5"/>
                    <object type="primitive" name="sphere">
                        <diffuse r="0.9" g="0.9" b="0.9" a="1"/>
                        <ambient r="0.9" g="0.9" b="0.9" a="1"/>
                        <specular r="1" g="1" b="1" a="1"/>
                    </object>
                </transblock>

                <!-- Right Arm (Cylinder) -->
                <transblock>
   
    <translate x="1" y="0.5" z="0"/> 

    <rotate  x="0" y="1" z="0" angle="60"/>
    <rotate x="1" y="0" z="0" angle="40"/>
    

   
    <scale x="0.1" y="1" z="0.1"/> 

    <object type="primitive" name="cylinder">
        <diffuse r="0.5" g="0.3" b="0.1" a="1"/>
        <ambient r="0.5" g="0.3" b="0.1" a="1"/>
        <specular r="0.5" g="0.3" b="0.1" a="1"/>
    </object>
</transblock>
               <!-- Left Arm (Cylinder) -->
<transblock>
    <translate x="-1" y="0.2" z="-0.1"/> 

    
    <rotate x="0" y="1" z="0" angle="-60"/> 
    <rotate x="1" y="0" z="0" angle="40"/> 

    <scale x="0.1" y="1" z="0.1"/> 

    <object type="primitive" name="cylinder">
        <diffuse r="0.5" g="0.3" b="0.1" a="1"/>
        <ambient r="0.5" g="0.3" b="0.1" a="1"/>
        <specular r="0.5" g="0.3" b="0.1" a="1"/>
    </object>
</transblock>
</object>

</transblock>
<!-- Top sphere (head) -->
<transblock>
    <translate x="0" y="2.5" z="0"/> <!-- Move above the middle sphere -->
    <scale x="1" y="1" z="1"/>
    <object type="primitive" name="sphere">
        <diffuse r="0.9" g="0.9" b="0.9" a="1"/>
        <ambient r="0.9" g="0.9" b="0.9" a="1"/>
        <specular r="1" g="1" b="1" a="1"/>
    </object>
</transblock>

<!-- Buttons on the middle sphere -->
<transblock>
    <translate x="0" y="1.9" z="0.65"/>
    <scale x="0.2" y="0.2" z="0.2"/>
    <object type="primitive" name="sphere">
        <diffuse r="0" g="0" b="0" a="1"/>
    </object>
</transblock>
<transblock>
    <translate x="0" y="1.5" z="0.65"/>
    <scale x="0.2" y="0.2" z="0.2"/>
    <object type="primitive" name="sphere">
        <diffuse r="0" g="0" b="0" a="1"/>
    </object>
</transblock>

<!-- Buttons on the bottom sphere -->
<transblock>
    <translate x="0" y="0.5" z="0.8"/>
    <scale x="0.2" y="0.2" z="0.2"/>
    <object type="primitive" name="sphere">
        <diffuse r="0" g="0" b="0" a="1"/>
    </object>
</transblock>
<transblock>
    <translate x="0" y="0.0" z="0.9"/>
    <scale x="0.2" y="0.2" z="0.2"/>
    <object type="primitive" name="sphere">
        <diffuse r="0" g="0" b="0" a="1"/>
    </object>
</transblock>

<!-- Eyes on the top sphere -->
<transblock>
    <translate x="0.0" y="2.8" z="0.5"/>
    <scale x="0.1" y="0.1" z="0.1"/>
    <object type="primitive" name="sphere">
        <diffuse r="0" g="0" b="0" a="1"/>
    </object>
</transblock>
<transblock>
    <translate x="0.6" y="2.8" z="0.5"/>
    <scale x="0.1" y="0.1" z="0.1"/>
    <object type="primitive" name="sphere">
        <diffuse r="0" g="0" b="0" a="1"/>
    </object>
</transblock>

<!-- Nose (small orange sphere for a carrot) -->
<transblock>
    <translate x="0.2" y="2.6" z="0.6"/>
    <scale x="0.15" y="0.15" z="0.4"/>
    <object type="primitive" name="sphere">
        <diffuse r="1" g="0.5" b="0" a="1"/>
    </object>
</transblock>






  
  </object>



</scenefile>



